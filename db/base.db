record(stringin, "$(P):ID_RD") {
    field(DESC, "getID")
    field(PINI, YES)
    field(DTYP, "stream")
    field(INP,  "@test.proto getID($(P)) dev1")
}

record(stringin, "$(P):MODEL_RD") {
    field(DESC, "get the model name")
}

record(stringin, "$(P):SERIAL_RD") {
    field(DESC, "get the serial number")
}

record(stringin, "$(P):FIRMWARE_RD") {
    field(DESC, "get the firmware version")
}

record(ao, "$(P):SYSBEEP_CMD") {
    field(DESC, "syst:beep")
    field(DTYP, "stream")
    field(OUT,  "@test.proto systBEEP dev1")
}
